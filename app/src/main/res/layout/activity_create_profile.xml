<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dark_lavender2"
    tools:context=".Activities.CreateProfile">

    <TextView
        android:id="@+id/Url"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="27dp"
        android:layout_marginTop="406dp"
        android:layout_marginEnd="298dp"
        android:layout_marginBottom="287dp"
        android:text="Url"
        android:textColor="@color/pale_pink"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="190dp" />

    <TextView
        android:id="@+id/Qualification"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="344dp"
        android:layout_marginEnd="295dp"
        android:layout_marginBottom="349dp"
        android:text="Qualification"
        android:textColor="@color/pale_pink"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="190dp" />

    <ImageView
        android:id="@+id/Profilepic"
        android:layout_width="105dp"
        android:layout_height="95dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="147dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="158dp"
        android:layout_marginBottom="596dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.05"
        app:srcCompat="@drawable/profile" />

    <EditText
        android:id="@+id/EnterPrice"
        android:layout_width="170dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="148dp"
        android:layout_marginTop="177dp"
        android:layout_marginEnd="92dp"
        android:layout_marginBottom="492dp"
        android:hint="Enter price"
        android:textColor="@color/pale_pink"
        android:textColorHint="@color/pale_pink"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.618"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.263" />

    <EditText
        android:id="@+id/EnterExperience"
        android:layout_width="170dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="150dp"
        android:layout_marginTop="249dp"
        android:layout_marginEnd="90dp"
        android:layout_marginBottom="420dp"
        android:hint="Enter your experience"
        android:textColor="@color/pale_pink"
        android:textColorHint="@color/pale_pink"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.618"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.263" />

    <EditText
        android:id="@+id/EnterQualification"
        android:layout_width="170dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="151dp"
        android:layout_marginTop="326dp"
        android:layout_marginEnd="89dp"
        android:layout_marginBottom="343dp"
        android:hint="Enter your qualification"
        android:textColor="@color/pale_pink"
        android:textColorHint="@color/pale_pink"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.618"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.263" />

    <EditText
        android:id="@+id/EnterURL"
        android:layout_width="170dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="151dp"
        android:layout_marginTop="388dp"
        android:layout_marginEnd="89dp"
        android:layout_marginBottom="281dp"
        android:hint="Enter your URL"
        android:textColor="@color/pale_pink"
        android:textColorHint="@color/pale_pink"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.618"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.263" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="86dp"
        android:layout_height="36dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="26dp"
        android:layout_marginTop="191dp"
        android:layout_marginEnd="299dp"
        android:layout_marginBottom="503dp"
        android:text="Price"
        android:textColor="@color/pale_pink"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="190dp" />

    <TextView
        android:id="@+id/Experience"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="26dp"
        android:layout_marginTop="262dp"
        android:layout_marginEnd="299dp"
        android:layout_marginBottom="431dp"
        android:text="Experience"
        android:textColor="@color/pale_pink"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="190dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/Enter"
        android:layout_width="111dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="156dp"
        android:layout_marginTop="476dp"
        android:layout_marginEnd="144dp"
        android:layout_marginBottom="207dp"
        android:backgroundTint="@color/lavender"
        android:text="Enter"
        app:cornerRadius="100dp" />

    <ProgressBar
        android:id="@+id/progressBar"
        android:layout_width="wrap_content"
        android:visibility="gone"
        android:layout_height="wrap_content"
        style="?android:attr/progressBarStyleLarge"
        android:layout_centerInParent="true"
        android:indeterminate="true"
        android:indeterminateTintMode="src_atop"
        android:indeterminateTint="@color/lavender" />
</RelativeLayout>